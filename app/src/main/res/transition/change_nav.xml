<?xml version="1.0" encoding="utf-8"?>
<transitionSet xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    >
    <targets>
        <target android:excludeClass="com.google.android.material.appbar.AppBarLayout" />
    </targets>
    <fade>
        <targets>
            <target android:excludeClass="com.google.android.material.floatingactionbutton.FloatingActionButton" />
        </targets>
    </fade>
    <transitionSet>
        <transition
            class="ru.uxapps.vocup.transition.ScaleVisibility"
            tools:ignore="Instantiatable"
            />
        <targets>
            <target android:targetClass="com.google.android.material.floatingactionbutton.FloatingActionButton" />
        </targets>
    </transitionSet>
</transitionSet>