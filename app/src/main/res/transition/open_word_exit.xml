<?xml version="1.0" encoding="utf-8"?>
<transitionSet xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <transitionSet>
        <explode android:duration="250"/>
        <fade android:duration="300" />
        <targets>
            <target android:excludeName="@string/trans_dict_add" />
            <target android:excludeName="@string/trans_dict_toolbar" />
        </targets>
    </transitionSet>
    <slide android:slideEdge="top" android:startDelay="50" android:duration="300">
        <targets>
            <target android:targetName="@string/trans_dict_toolbar" />
        </targets>
    </slide>
    <transitionSet>
        <transition
            class="ru.uxapps.vocup.transitions.ScaleVisibilityTransition"
            tools:ignore="Instantiatable"
            />
        <targets>
            <target android:targetName="@string/trans_dict_add" />
        </targets>
    </transitionSet>
</transitionSet>